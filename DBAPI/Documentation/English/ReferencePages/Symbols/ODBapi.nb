  3.659695915154457*^9, 3.659695919672035*^9}},
 CellID->34439295],

Cell[BoxData[
 RowBox[{
  RowBox[{"john", "=", 
   RowBox[{"{", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\"\<telephones\>\"", "\[Rule]", 
      RowBox[{"Thread", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
         "\"\<home\>\"", ",", "\"\<business\>\"", ",", " ", 
          "\"\<mobile\>\""}], "}"}], "\[Rule]", 
        RowBox[{"{", 
         RowBox[{
         "\"\<2104566345\>\"", ",", "\"\<2108856844\>\"", ",", 
          "\"\<6974059256\>\""}], "}"}]}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"\"\<firstName\>\"", "\[Rule]", "\"\<John\>\""}], ",", " ", 
     RowBox[{"\"\<lastName\>\"", "\[Rule]", "\"\<Brown\>\""}], ",", 
     RowBox[{"\"\<DOB\>\"", "\[Rule]", " ", "\"\<1971-10-01\>\""}], ",", 
     RowBox[{"\"\<age\>\"", "\[Rule]", "44"}]}], "}"}]}], ";"}]], "Input",
