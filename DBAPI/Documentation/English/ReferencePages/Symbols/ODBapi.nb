 StyleBox[ButtonBox["Utilities Package",
  BaseStyle->"Link",
  ButtonData->"paclet:DBAPI/guide/UtilitiesPackage"],
  FontWeight->"Bold",
  Background->RGBColor[1, 0.9, 0.8]],
 StyleBox[".",
  FontWeight->"Bold",
